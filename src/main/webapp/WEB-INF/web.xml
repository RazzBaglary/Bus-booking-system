<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>register</servlet-name>
		<display-name>register</display-name>
		<description></description>
		<servlet-class>com.servlet.register</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>login</servlet-name>
		<display-name>login</display-name>
		<description></description>
		<servlet-class>com.servlet.login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>logout</servlet-name>
		<display-name>logout</display-name>
		<description></description>
		<servlet-class>com.servlet.logout</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>adminsignin</servlet-name>
		<display-name>adminsignin</display-name>
		<description></description>
		<servlet-class>com.servlet.adminsignin</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>addbus</servlet-name>
		<display-name>addbus</display-name>
		<description></description>
		<servlet-class>com.servlet.addbus</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>deleteBus</servlet-name>
		<display-name>deleteBus</display-name>
		<description></description>
		<servlet-class>com.servlet.deleteBus</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>bsearch</servlet-name>
		<display-name>bsearch</display-name>
		<description></description>
		<servlet-class>com.servlet.bsearch</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>adminsignin</servlet-name>
		<url-pattern>/adminsignin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>addbus</servlet-name>
		<url-pattern>/addbus</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>deleteBus</servlet-name>
		<url-pattern>/deleteBus</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>bsearch</servlet-name>
		<url-pattern>/bsearch</url-pattern>
	</servlet-mapping>
	

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
